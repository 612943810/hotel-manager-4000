@using Hotel_Manager_4000.Areas.Owner.Models;
@model List<Room>
@{
    Layout = "_OwnerLayout";
}

<table class="table table-striped table-danger">
    <thead>
        <tr>
            <th>Room Number</th>
            <th>Room Level</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var roomData in @Model)
        {
            <tr>
                <td>@roomData.RoomNumber</td>
                <td>@roomData.RoomLevel</td>
            </tr>
    
        }
    </tbody>
</table>