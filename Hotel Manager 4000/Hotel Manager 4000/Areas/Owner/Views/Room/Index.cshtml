@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Hotel_Manager_4000.Areas.Owner.Models;
<a asp-area ="Owner" asp-controller="Room" asp-action="CreateRoom">
    <button class="btn btn-warning" type="submit">Create Room</button>
</a>
<table class="table table-striped table-danger">
    <thead>
        <tr>
            <th>Room Number</th>
            <th>Room Level</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var roomData in @Model)
        {
            <tr>
                <td>@roomData.RoomNumber</td>
                <td>@roomData.RoomLevel</td>
            </tr>
    
        }
    </tbody>
</table>